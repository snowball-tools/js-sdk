{
	"name": "@snowballtools/auth-embedded",
	"version": "0.4.2",
	"description": "snowballtools auth-passkey",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"browser": {
		"./dist/index.mjs": "./dist/index.browser.mjs"
	},
	"types": "./dist/index.d.ts",
	"files": [
